#!/usr/bin/make -f

VERSION=1.67.1

%:
	dh $@

override_dh_auto_clean:
	ischroot || bash fetch.sh upstream $(VERSION) \
		652a8966436c4e97b127721d9130810e1cdc8dfdf526fad68c9c1f6281bd02a3 \
		8edee248eed4b17c09b3d7b0096944b7e5992dd1119a28429c0b6b4d39a9613c
#	ischroot || just version=$(VERSION) fetch

override_dh_auto_build:
	just version=$(VERSION)

override_dh_auto_install:
	just version=$(VERSION) rootdir=debian/tmp install

override_dh_strip:

override_dh_shlibdeps:
	dh_shlibdeps -Xclippy