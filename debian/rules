#!/usr/bin/make -f

VERSION=1.73.0

%:
	dh $@

override_dh_auto_clean:
	ischroot || bash fetch.sh upstream $(VERSION) \
		aa4cf0b7e66a9f5b7c623d4b340bb1ac2864a5f2c2b981f39f796245dc84f2cb \
		e54d7d886ba413ae573151f668e76ea537f9a44406d3d29598269a4a536d12f6
#	ischroot || just version=$(VERSION) fetch

override_dh_auto_build:
	just version=$(VERSION)

override_dh_auto_install:
	just version=$(VERSION) rootdir=debian/tmp install

override_dh_strip:

override_dh_shlibdeps:
	dh_shlibdeps -Xclippy